        <hashTree>
          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Assert Exactly One Winner" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">int threads = Integer.parseInt(vars.get(&quot;raceThreads&quot;))
int success = Integer.parseInt(props.getProperty(&quot;raceSuccess&quot;, &quot;0&quot;))
int rejected = Integer.parseInt(props.getProperty(&quot;raceDuplicate&quot;, &quot;0&quot;))
int unexpected = Integer.parseInt(props.getProperty(&quot;raceUnexpected&quot;, &quot;0&quot;))

if (success != 1 || rejected != (threads - 1) || unexpected != 0) {
    throw new AssertionError(
            &quot;Uniqueness race failed: success=&quot; + success +
                    &quot;, duplicate=&quot; + rejected +
                    &quot;, unexpected=&quot; + unexpected +
                    &quot;, threads=&quot; + threads
    )
}</stringProp>
