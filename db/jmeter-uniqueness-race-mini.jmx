        <hashTree>
          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="Assert Allowed Outcome" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">String code = prev.getResponseCode()
if (!(&quot;201&quot;.equals(code) || &quot;400&quot;.equals(code))) {
    AssertionResult.setFailure(true)
    AssertionResult.setFailureMessage(&quot;Expected 201 or 400, got &quot; + code)
    return
}
if (&quot;400&quot;.equals(code)) {
    String body = (prev.getResponseDataAsString() ?: &quot;&quot;).toLowerCase()
    if (!(body.contains(&quot;must be unique&quot;) || body.contains(&quot;уникаль&quot;))) {
        AssertionResult.setFailure(true)
        AssertionResult.setFailureMessage(&quot;400 without uniqueness message: &quot; + prev.getResponseDataAsString())
    }
}</stringProp>
          </JSR223Assertion>
          <hashTree/>
          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Count Outcomes" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">String code = prev.getResponseCode()
String body = (prev.getResponseDataAsString() ?: &quot;&quot;).toLowerCase()
boolean duplicate = &quot;400&quot;.equals(code) &amp;&amp; (body.contains(&quot;must be unique&quot;) || body.contains(&quot;уникаль&quot;))

synchronized (props) {
    int success = Integer.parseInt(props.getProperty(&quot;raceSuccess&quot;, &quot;0&quot;))
    int rejected = Integer.parseInt(props.getProperty(&quot;raceDuplicate&quot;, &quot;0&quot;))
    int unexpected = Integer.parseInt(props.getProperty(&quot;raceUnexpected&quot;, &quot;0&quot;))

    if (&quot;201&quot;.equals(code)) {
        success++
    } else if (duplicate) {
        rejected++
    } else {
        unexpected++
    }

    props.put(&quot;raceSuccess&quot;, String.valueOf(success))
    props.put(&quot;raceDuplicate&quot;, String.valueOf(rejected))
    props.put(&quot;raceUnexpected&quot;, String.valueOf(unexpected))
}</stringProp>
          </JSR223PostProcessor>
          <hashTree/>
