props.put(&quot;raceEventId&quot;, &quot;&quot;)
props.put(&quot;raceSuccess&quot;, &quot;0&quot;)
props.put(&quot;raceDuplicate&quot;, &quot;0&quot;)
props.put(&quot;raceUnexpected&quot;, &quot;0&quot;)</stringProp>
